{
  "name": "vue-material-symbols",
  "private": false,
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "prepublishOnly": "npm run build",
    "preview": "vite preview",
    "generate-types": "node scripts/fetchFonts.js",
    "test": "vitest"
  },
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/vue-material-symbols.umd.js",
  "module": "dist/vue-material-symbols.es.js",
  "types": "types/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-material-symbols.es.js",
      "require": "./dist/vue-material-symbols.umd.js",
      "types": "./types/index.d.ts"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "2.0.0-rc.18",
    "happy-dom": "^6.0.4",
    "node-fetch": "^3.2.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.5",
    "vitest": "^0.21.1",
    "vue": "^3.2.25",
    "vue-tsc": "^0.34.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsutoringo/vue-material-symbols.git"
  },
  "bugs": {
    "url": "https://github.com/tsutoringo/vue-material-symbols/issues"
  },
  "homepage": "https://github.com/tsutoringo/vue-material-symbols#readme",
  "keywords": [
    "vue3",
    "vue",
    "icon",
    "font"
  ],
  "license": "MIT"
}
