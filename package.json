{
  "name": "vue-material-symbols",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "generate-types": "node scripts/fetchFonts.js"
  },
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/vue-material-symbols.umd.js",
  "module": "dist/vue-material-symbols.es.js",
  "types": "types/components/index.d.ts",
  "source": "src/components/index.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-material-symbols.es.js",
      "require": "./dist/vue-material-symbols.umd.js",
      "types": "./types/components/index.d.ts"
    }
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "node-fetch": "^3.2.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.17"
  }
}
